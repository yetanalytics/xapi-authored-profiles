            "inScheme": "https://registry.tincanapi.com",
            "prefLabel": {
                "en": "discussion"
            },
            "definition": {
                "en": "Represents an ongoing conversation between persons, such as an email thread or a forum topic."
            }
        },
        {
            "id": "http://id.tincanapi.com/activitytype/document",
            "type": "ActivityType",
            "inScheme": "https://registry.tincanapi.com",
            "prefLabel": {
                "en": "document"
            },
